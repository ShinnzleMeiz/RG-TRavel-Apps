<h1>Listing registrationns</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Email</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registrationns.each do |registrationn| %>
      <tr>
        <td><%= registrationn.name %></td>
        <td><%= registrationn.age %></td>
        <td><%= registrationn.email %></td>
        <td><%= registrationn.address %></td>
        <td><%= link_to 'Show', registrationn %></td>
        <td><%= link_to 'Edit', edit_registrationn_path(registrationn) %></td>
        <td><%= link_to 'Destroy', registrationn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

